<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="co.yedam.silhyun.common.map.CommentMapper">
	
	<!-- 댓글 리스트 -->
	<select id="getCommentList" parameterType="CommentVO" resultType="co.yedam.silhyun.common.vo.CommentVO">
		select id, cntn, com_date
		from commment
		where ctgr_num = 12
		and dep = 0
		order by grp, dep, com_num
	</select>
	<insert id=""></insert>	
</mapper>